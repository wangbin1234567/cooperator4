/* pages/habit/habit.wxss */
.target-box{
  width:100%;
  height:100%;
}
.title{
  width:100%;
  height:80rpx;
  display: flex;
  border-bottom:1px solid #ccc;
}
.title input{
  width:80%;
}

.title text{
  width:20%;
  height:60rpx;
   margin-left:10rpx;
  line-height:60rpx;
  text-align: center;
  color:#000;
  border:1px solid #ccc;
  border-radius: 5rpx;
}
.list{
  width:100%;
  height:100rpx;
  display: flex;
  align-content: center;
  background:#ccc;
  margin-top:30rpx;
  border-radius: 10rpx;
}
.list text:nth-of-type(1){
  width:70%;
  line-height:100rpx;
}
.list text:nth-of-type(2){
  width:30%;
  line-height:100rpx;
  font-size:50rpx;
  color:#000;
}