/* pages/target/target.wxss */
.wraps{
  width: 100%;
  height: 1200rpx;
  display: flex;
  flex-direction: column;
}
.ipt{
  height: 100rpx;
  display: flex;
}
.list{
  border-top: 2rpx solid #ccc;
  flex: 1;
  background: #eee;
  padding: 0 30rpx;
}
.cons{
  background: #fff;
  padding: 20rpx 50rpx;
}
.cons view{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 20rpx;
  background: #eee;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
  padding: 0 20rpx;
}